  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.15.0",
    "@jupyterlab/codemirror": "^0.15.0",
    "@jupyterlab/cells": "^0.15.0",
    "@jupyterlab/notebook": "^0.15.0",
    "@phosphor/commands": "^1.4.0",
    "@types/codemirror": "^0.0.55"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "typescript": "~2.6.2"
